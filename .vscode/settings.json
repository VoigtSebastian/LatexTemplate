{
  "latex-workshop.latex.recipes": [
    {
      "name": "pdflatex, biber, makeglossaries, pdflatex*2",
      "tools": [
        "pdflatex",
        "biber",
        "makeglossaries",
        "pdflatex",
        "pdflatex"
      ]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"]
    },
    {
      "name": "biber",
      "command": "biber",
      "args": ["%DOCFILE%"]
    },
    {
      "name": "makeglossaries",
      "command": "makeglossaries",
      "args": ["%DOCFILE%"]
    }
  ],
  "latex-workshop.latex.recipe.default": "lastUsed"
}
